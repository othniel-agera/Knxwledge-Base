{
	"name": "knxwledge-base",
	"version": "1.0.0",
	"description": "",
	"scripts": {
		"start": "node ./dist/index.js",
		"dev": "nodemon",
		"test": "mocha -r ts-node/register tests/**/*.spec.ts",
		"test:coverage": "nyc npm run test"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@types/mochawesome": "^6.2.1",
		"dotenv": "^16.3.1",
		"express": "^4.18.2"
	},
	"devDependencies": {
		"@istanbuljs/nyc-config-typescript": "^1.0.2",
		"@types/chai": "^4.3.5",
		"@types/dotenv": "^8.2.0",
		"@types/express": "^4.17.17",
		"@types/mocha": "^10.0.1",
		"@types/node": "^20.5.4",
		"chai": "^4.3.8",
		"mocha": "^10.2.0",
		"nodemon": "^3.0.1",
		"nyc": "^15.1.0",
		"ts-node": "^10.9.1",
		"tsc": "^2.0.4",
		"typescript": "^5.2.2"
	},
	"nyc": {
		"extends": "@istanbuljs/nyc-config-typescript",
		"check-coverage": true,
		"all": true,
		"include": [
			"src/**/!(*.test.*).[tj]s?(x)"
		],
		"exclude": [
			"src/_tests_/**/*.*"
		],
		"reporter": [
			"html",
			"lcov",
			"text",
			"text-summary"
		],
		"report-dir": "coverage"
	}
}
